        <div class="breadcrumbs">
            <div class="col-sm-4">
                <div class="page-header float-left">
                    <div class="page-title">
                        <h1 id="dbtest">Dashboard</h1>
                    </div>
                </div>
            </div>
            <div class="col-sm-8">
                <div class="page-header float-right">
                    <div id="status_connect_admin">
                    </div>
                </div>
            </div>
        </div>

        <div id="content_right_panel" class="content mt-3">

            <div class="col-sm-12">
                <div class="alert  alert-success alert-dismissible fade show" role="alert">
                  <span class="badge badge-pill badge-success">Success</span> You successfully read this important alert message.
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
            </div>


           <div class="col-sm-6 col-lg-3">
                <div class="card text-white bg-flat-color-1">
                    <div class="card-body pb-0">
                        <h4 class="mb-0">
                            <span class="count">{{amount_users}}</span>
                        </h4>

                        <div class="chart-wrapper px-0" style="height:70px;" height="70">
                            <img class="d-inline" src="../static/public/images/admin/users_ic_card.png" alt="ic_users"/>  
                            <h2 class="d-inline float-right mt-4">Users</h2>                
                        </div>

                    </div>

                </div>
            </div>
            <!--/.col-->

            <div class="col-sm-6 col-lg-3">
                <div class="card text-white bg-flat-color-2">
                    <div class="card-body pb-0">
                        <h4 class="mb-0">
                            <span class="count">{{amount_songs}}</span>
                        </h4>
                        <div class="chart-wrapper px-0" style="height:70px;" height="70">
                            <img style="width: 64px; height:auto;" class="d-inline" src="../static/public/images/admin/song_ic_card.png" alt="ic_song"/>  
                            <h2 class="d-inline float-right mt-4">Song</h2>         
                        </div>

                    </div>
                </div>
            </div>
            <!--/.col-->

            <div class="col-sm-6 col-lg-3">
                <div class="card text-white bg-flat-color-3">
                    <div class="card-body pb-0">
                        <h4 class="mb-0">
                            <span class="count">{{amount_singers}}</span>
                        </h4>
                    </div>

                        <div class="chart-wrapper px-0" style="height:70px;" height="70">
                            <img class="d-inline ml-4" src="../static/public/images/admin/singer_ic_card.png" alt="ic_singer"/>  
                            <h2 class="d-inline float-right mt-4 mr-2">Singer</h2>        
                        </div>
                </div>
            </div>
            <!--/.col-->

            <div class="col-sm-6 col-lg-3">
                <div class="card text-white bg-flat-color-4">
                    <div class="card-body pb-0">
                        <h4 class="mb-0">
                            <span class="count">{{amount_albums}}</span>
                        </h4>

                        <div class="chart-wrapper px-3" style="height:70px;" height="70">
                            <img style="width: 64px; height:auto;" class="d-inline" src="../static/public/images/admin/album_ic_card.png" alt="ic_album"/>  
                            <h2 class="d-inline float-right mt-4">Album</h2>    
                        </div>

                    </div>
                </div>
            </div>
            <!--/.col-->

            <div class="col-sm-6 col-lg-3">
                    <div class="card text-white bg-flat-color-4">
                        <div class="card-body pb-0">
                            <h4 class="mb-0">
                                <span class="count">{{amount_lyrics}}</span>
                            </h4>
    
                            <div class="chart-wrapper px-3" style="height:70px;" height="70">
                                <img style="width: 64px; height:auto;" class="d-inline" src="../static/public/images/admin/lyric_ic_card.png" alt="ic_lyric"/>  
                                <h2 class="d-inline float-right mt-4">Lyric</h2>    
                            </div>
    
                        </div>
                    </div>
                </div>
                <!--/.col-->


        </div> <!-- .content -->
    </div><!-- /#right-panel -->

